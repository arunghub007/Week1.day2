package week1.day2;

import java.util.Arrays;


public class ArrayMissingElement {
	public static void main(String[] args) {
		int[] x = {1,2,3,4,7,6,8};
		Arrays.sort(x);
		int count = 1;
		int i;
		for(i=0;i<x.length;i++) {
			if(x[i]!= i+count ) {
		
				System.out.println(i+count);
				count ++;
				
				
		}
		
		}
		
		
	}

}

